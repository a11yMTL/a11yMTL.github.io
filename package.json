{
  "name": "a11yMTL",
  "version": "1.0.0",
  "description": "a11yMTL website built with the base eleventy template",
  "scripts": {
    "build": "npm-run-all sass-build --parallel eleventy",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "sass-start": "sass scss/styles.scss _site/css/a11ymtl.css",
    "sass-build": "sass --style=compressed scss/styles.scss _site/css/a11ymtl.css",
    "eleventy": "npx eleventy",
    "watch:eleventy": "npx @11ty/eleventy --watch",
    "serve": "npx @11ty/eleventy --serve",
    "start": "npm-run-all sass-start --parallel watch:*",
    "debug": "DEBUG=* npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/accessibility/a11ymtl.git"
  },
  "author": {
    "name": "a11yMTL",
    "email": "a11ymtl@gmail.com",
    "url": "a11ymtl.net"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/accessibility/a11ymtl/issues"
  },
  "homepage": "https://github.com/accessibility/a11ymtl#readme",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "luxon": "^2.3.1",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.49.9"
  }
}
